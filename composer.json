{
    "name": "freshinup/smartmotors",
    "type": "project",
    "version": "1.0.0",
    "description": "SmartMotors.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "dompdf/dompdf": "^0.8.4",
        "elasticsearch/elasticsearch": "^7.4",
        "fideloper/proxy": "^4.0",
        "freshinup/activity-api": "1.13.1",
        "freshinup/fresh-bus-forms": "2.53.3",
        "freshinup/fresh-deals-api": "1.17.0",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "ddavaham/laravel-env-set-command": "dev-update-empty-variable",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.15",
        "fzaninotto/faker": "^1.4",
        "jjclane/laravel-sqlite-migrations": "^0.1.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5",
        "rregeer/phpunit-coverage-check": "^0.2.1",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "lint": "vendor/bin/phpcs --ignore=database/migrations/** && composer lint2",
        "lint:fix": "vendor/bin/phpcbf && composer lint2:fix",
        "lint2": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist -v --dry-run --using-cache=no --diff",
        "lint2:fix": "vendor/bin/php-cs-fixer fix --config=.php_cs.dist -v --using-cache=no --diff",
        "test:coverage-check": "vendor/bin/coverage-check tests/.reports/coverage/clover.xml 80",
        "test": "composer test:all-coverage && composer test:coverage-check",
        "test:all-coverage": [
            "touch storage/testing.sqlite",
            "phpdbg -qrr vendor/bin/phpunit --configuration phpunit.xml --testsuite Package -d memory_limit=512M"
        ],
        "test:feature": [
            "touch storage/testing.sqlite",
            "vendor/bin/phpunit --configuration phpunit.xml --testsuite Feature --no-coverage -d memory_limit=512M"
        ],
        "test:unit": [
            "touch storage/testing.sqlite",
            "vendor/bin/phpunit --configuration phpunit.xml --no-coverage --testsuite Unit -d memory_limit=256M"
        ],
        "test:smoke": [
            "touch storage/testing.sqlite",
            "vendor/bin/phpunit --configuration phpunit.xml --testsuite Smoke --no-coverage -d memory_limit=512M"
        ]
    },
    "repositories": {
        "fresh-bus-forms": {
            "type": "vcs",
            "url": "git@github.com:FreshinUp/fresh-bus-forms.git"
        },
        "ddavaham/laravel-env-set-command": {
            "type": "vcs",
            "url": "git@github.com:ddavaham/laravel-env-set-command.git"
        },
        "fresh-deals-api": {
            "type": "vcs",
            "url": "git@github.com:FreshinUp/fresh-deals-api.git"
        },
        "activity-api": {
            "type": "vcs",
            "url": "git@github.com:FreshinUp/activity-api.git"
        }
    }
}
